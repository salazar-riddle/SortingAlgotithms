void Solution::sortColors(vector<int> &A) {
    int count0=0,count1=0,count2=0;
    for(int i=0;i<A.size();i++){
        if(A[i]==0) count0++;
        else if(A[i]==1) count1++;
        else if(A[i]==2) count2++;
    }
    int i=0;
    for( i=0;i<count0;i++){
        A[i]=0;
    }

    for(;i<count1+count0;i++){
        A[i]=1;
    }
    for(;i<count2+count1+count0;i++){
        A[i]=2;
    }
}
